<!DOCTYPE html>
<html>
<head>
  <title>Biddr</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <ul class="nav nav-tabs">
    <li role="presentation" class="active"><%= link_to "HOME", root_path %></li>
    <li role="presentation"><%= link_to "Sign-UP", new_user_path %></li>
    <li role="presentation"><%= link_to "Sign-IN", new_session_path %></li>
    <li role="presentation"><%= link_to "SIGN OUT", sessions_path, method: :delete %></li>
    <li role="presentation"><%= link_to "Auction", new_auction_path %></li>
  </ul>



 
<div>

<% if user_signed_in? %>
<p>Hi <%= current_user.email  %>
<% end %>
</p>

</div>
<%= yield %>

</body>
</html>
